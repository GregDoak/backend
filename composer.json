{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "data-dog/audit-bundle": "^0.1.10",
    "friendsofsymfony/rest-bundle": "^2.3",
    "gesdinet/jwt-refresh-token-bundle": "^0.4",
    "greg-doak/cron-bundle": "^1.0@dev",
    "jms/serializer-bundle": "^2.3",
    "lexik/jwt-authentication-bundle": "^2.4",
    "nelmio/cors-bundle": "^1.5",
    "sensio/framework-extra-bundle": "5.1.0",
    "sensiolabs/security-checker": "^4.1",
    "symfony/cache": "^4.0",
    "symfony/console": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/web-server-bundle": "^4.0",
    "symfony/yaml": "^4.0",
    "ua-parser/uap-php": "^3.5"
  },
  "require-dev": {
    "symfony/browser-kit": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/thanks": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "generate-environment-files": [
      "cp -n phpunit.xml.dist phpunit.xml"
    ],
    "post-install-cmd": [
      "@auto-scripts",
      "@generate-environment-files"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "@generate-environment-files"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C1WWR27ZWS5KK2ARRXXMX3JF",
      "allow-contrib": false
    }
  }
}
